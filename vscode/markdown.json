{
  "include": {
    "prefix": [
      "include",
      "inc_include"
    ],
    "body": [
      "[$1](include/$1.md ':include')"
    ],
    "description": "插入预定义内容片段（include/）"
  },
  "include_terms": {
    "prefix": [
      "include_terms",
      "inc_terms"
    ],
    "body": [
      "[$1](terms/$1.md ':include')"
    ],
    "description": "插入预定义内容片段（terms/）"
  },
  "include_terms_path": {
    "prefix": [
      "include_terms_path",
      "inc_terms_path"
    ],
    "body": [
      "[path](terms/path.md ':include')"
    ],
    "description": "`path`的定义说明"
  },
  "include_terms_host": {
    "prefix": [
      "include_terms_host",
      "inc_terms_host"
    ],
    "body": [
      "[host](terms/host.md ':include')"
    ],
    "description": "`host`的定义说明"
  },
  "git_search": {
    "prefix": "git_search",
    "body": "「[zblogcn/zblogphp/search?q=${1:内容}](https://github.com/zblogcn/zblogphp/search?q=${1:内容} \"Search · ${1:内容}\")」",
    "description": "链接至 Git 搜索"
  },
  "comer_brackets": {
    "prefix": [
      "||",
    ],
    "body": "「${1:内容}」",
    "description": "输入`「」`用于着重某些内容"
  },
  "code": {
    "prefix": [
      "&&",
    ],
    "body": " `${1:code}` ",
    "description": "行内代码`code`"
  },
  "**注：**": {
    "prefix": "**",
    "body": "**注：$1**",
    "description": "加粗内容「注：」"
  },
  "**重要：**": {
    "prefix": "**",
    "body": "**重要：$1**",
    "description": "加粗内容「重要：」"
  },
  "docsify": {
    "prefix": "docsify",
    "body": "docsify",
    "description": "docsify"
  },
  "more": {
    "prefix": "more",
    "body": "<!--more-->",
    "description": "摘要分隔"
  }
}